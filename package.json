{
  "name": "polymath-contract-wrappers",
  "version": "0.0.0",
  "description": "Smart TS wrappers for Polymath smart contracts",
  "keywords": [
    "polymath",
    "ethereum"
  ],
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "build:ci": "yarn build",
    "lint": "tslint --format stylish --project . --exclude **/lib/**/*",
    "clean": "shx rm -rf lib"
  },
  "config": {
    "postpublish": {
      "assets": []
    }
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.104"
  },
  "dependencies": {
    "@0x/assert": "^1.0.21",
    "@0x/json-schemas": "^2.1.5",
    "@0x/typescript-typings": "^3.0.6",
    "@0x/web3-wrapper": "^3.2.2",
    "ethereum-types": "^1.1.4",
    "polymath-contract-artifacts": "git://github.com/PolymathNetwork/polymath-contract-artifacts.git#v2.1.0-beta",
    "polymath-abi-wrappers": "git://github.com/PolymathNetwork/polymath-abi-wrappers.git#v2.1.0-beta",
    "lodash": "^4.17.5"
  },
  "publishConfig": {
    "access": "public"
  }
}